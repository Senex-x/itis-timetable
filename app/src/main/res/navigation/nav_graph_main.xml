<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/scheduleFragment">

    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.senex.timetable.presentation.ui.schedule.ScheduleFragment"
        tools:layout="@layout/fragment_schedule">
        <action
            android:id="@+id/action_scheduleFragment_to_groupsFragment"
            app:destination="@id/groupsFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_ordinarySubjectFragment"
            app:destination="@id/ordinarySubjectFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_electiveSubjectFragment"
            app:destination="@id/electiveSubjectFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_hiddenSubjectsFragment"
            app:destination="@id/hiddenSubjectsFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_englishSubjectFragment"
            app:destination="@id/englishSubjectFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_newElectiveSubjectFragment"
            app:destination="@id/newElectiveSubjectFragment" />
        <action
            android:id="@+id/action_scheduleFragment_to_newEnglishSubjectFragment"
            app:destination="@id/newEnglishSubjectFragment" />
    </fragment>
    <fragment
        android:id="@+id/groupsFragment"
        android:name="com.senex.timetable.presentation.ui.groups.GroupsFragment"
        android:label="fragment_groups"
        tools:layout="@layout/fragment_groups">
        <action
            android:id="@+id/action_groupsFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/ordinarySubjectFragment"
        android:name="com.senex.timetable.presentation.ui.subject.ordinary.OrdinarySubjectFragment"
        android:label="SubjectFragment"
        tools:layout="@layout/fragment_ordinary_subject">
        <action
            android:id="@+id/action_ordinarySubjectFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <argument
            android:name="subject_id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/electiveSubjectFragment"
        android:name="com.senex.timetable.presentation.ui.subject.elective.ElectiveSubjectFragment"
        android:label="ElectiveSubjectFragment"
        tools:layout="@layout/fragment_elective_subject">
        <action
            android:id="@+id/action_electiveSubjectFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <argument
            android:name="elective_subject_id"
            app:argType="long" />
        <action
            android:id="@+id/action_electiveSubjectFragment_to_selectableElectiveSubjectsFragment"
            app:destination="@id/selectableElectiveSubjectsFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectableElectiveSubjectsFragment"
        android:name="com.senex.timetable.presentation.ui.subject.elective.selectable.SelectableElectiveSubjectsFragment"
        android:label="SelectableElectiveSubjectsFragment"
        tools:layout="@layout/fragment_selectable_elective_subjects">
        <action
            android:id="@+id/action_selectableElectiveSubjectsFragment_to_electiveSubjectFragment"
            app:destination="@id/electiveSubjectFragment" />
        <argument
            android:name="elective_subject_id"
            app:argType="long" />
        <argument
            android:name="primary_elective_subject_id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/englishSubjectFragment"
        android:name="com.senex.timetable.presentation.ui.subject.english.EnglishSubjectFragment"
        android:label="EnglishSubjectFragment"
        tools:layout="@layout/fragment_english_subject">
        <action
            android:id="@+id/action_englishSubjectFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <argument
            android:name="english_subject_id"
            app:argType="long" />
        <action
            android:id="@+id/action_englishSubjectFragment_to_selectableEnglishSubjectsFragment"
            app:destination="@id/selectableEnglishSubjectsFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectableEnglishSubjectsFragment"
        android:name="com.senex.timetable.presentation.ui.subject.english.selectable.SelectableEnglishSubjectsFragment"
        android:label="SelectableEnglishSubjectsFragment"
        tools:layout="@layout/fragment_selectable_english_subjects">
        <action
            android:id="@+id/action_selectableEnglishSubjectsFragment_to_englishSubjectFragment"
            app:destination="@id/englishSubjectFragment" />
        <argument
            android:name="english_subject_id"
            app:argType="long" />
        <argument
            android:name="primary_english_subject_id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.senex.timetable.presentation.ui.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/hiddenSubjectsFragment"
        android:name="com.senex.timetable.presentation.ui.hidden.HiddenSubjectsFragment"
        android:label="HiddenSubjectsFragment"
        tools:layout="@layout/fragment_hidden_subjects">
        <action
            android:id="@+id/action_hiddenSubjectsFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
    </fragment>
    <fragment
        android:id="@+id/newElectiveSubjectFragment"
        android:name="com.senex.timetable.presentation.ui.subject.varied.elective.NewElectiveSubjectFragment"
        android:label="NewElectiveSubjectFragment"
        tools:layout="@layout/fragment_varied_subject">
        <action
            android:id="@+id/action_newElectiveSubjectFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <argument
            android:name="elective_subject_id"
            app:argType="long" />
        <action
            android:id="@+id/action_newElectiveSubjectFragment_to_newSelectableElectiveSubjectsFragment"
            app:destination="@id/newSelectableElectiveSubjectsFragment" />
    </fragment>
    <fragment
        android:id="@+id/newEnglishSubjectFragment"
        android:name="com.senex.timetable.presentation.ui.subject.varied.english.NewEnglishSubjectFragment"
        android:label="NewEnglishSubjectFragment"
        tools:layout="@layout/fragment_varied_subject">
        <action
            android:id="@+id/action_newEnglishSubjectFragment_to_scheduleFragment"
            app:destination="@id/scheduleFragment" />
        <argument
            android:name="english_subject_id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/newSelectableElectiveSubjectsFragment"
        android:name="com.senex.timetable.presentation.ui.subject.varied.elective.selectable.NewSelectableElectiveSubjectsFragment"
        android:label="NewSelectableElectiveSubjectsFragment" >
        <action
            android:id="@+id/action_newSelectableElectiveSubjectsFragment_to_newElectiveSubjectFragment"
            app:destination="@id/newElectiveSubjectFragment" />
        <argument
            android:name="elective_subject_id"
            app:argType="long" />
        <argument
            android:name="primary_elective_subject_id"
            app:argType="long" />
    </fragment>
</navigation>